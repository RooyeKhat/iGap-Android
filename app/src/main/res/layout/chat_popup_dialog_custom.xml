<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="wrap_content"
  android:background="@android:color/transparent"
  android:clickable="true"
  android:orientation="vertical">


  <LinearLayout
    android:id="@+id/dialog_root_item1_notification"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <net.iGap.module.MaterialDesignTextView
      android:id="@+id/dialog_icon_item1_notification"
      android:layout_width="@dimen/dp26"
      android:layout_height="match_parent"
      android:layout_gravity="center"
      android:gravity="center"
      android:text="@string/md_muted"
      android:textAppearance="@style/TextIconAppearance.all"
      style="?textSubColorSettingTheme"
      android:textSize="@dimen/dp18"

      />

    <TextView
      android:id="@+id/dialog_text_item1_notification"
      style="?textTitleColorSettingTheme"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="@string/mute_notification"
      android:paddingTop="@dimen/dp8"
      android:paddingBottom="@dimen/dp8"
      android:paddingLeft="@dimen/dp8"
      android:paddingRight="@dimen/dp8"
      android:textSize="@dimen/dp14" />

  </LinearLayout>

